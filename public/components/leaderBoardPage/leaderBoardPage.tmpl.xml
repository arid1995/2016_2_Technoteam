<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
  <table>
  <thead>
    <tr>
      <th>Login</th>
      <th>Score</th>
    </tr>
  </thead>
  <tbody>
    <fest:for iterate="data.userList" index="i" value="user">
				<tr>
          <td><fest:value output="js">user.login</fest:value></td>
          <td><fest:value output="js">user.score</fest:value></td>
        </tr>
		</fest:for>
  </tbody>
</table>
</fest:template>
